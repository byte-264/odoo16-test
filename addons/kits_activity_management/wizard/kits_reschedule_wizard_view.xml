<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="kits_reschedule_wizard_view_form" model="ir.ui.view">
        <field name="name">kits.reschedule.wizard.view.form</field>
        <field name="model">kits.reschedule.wizard</field>
        <field name="arch" type="xml">
            <form string="Reschedule Activity">
                <header>
                </header>
                <sheet>
                    <group>
                        <group>
                            <field name="reschedule_date"/>
                        </group>
                    </group>
                </sheet>
                <footer>
                    <button name="action_reschedule" class="btn-primary" string="Reschedule" type="object"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>